{
  "globalStyle": {
    "navigationStyle": "default",
    "navigationBarTitleText": "unibest",
    "navigationBarBackgroundColor": "#f8f8f8",
    "navigationBarTextStyle": "black",
    "backgroundColor": "#FFFFFF"
  },
  "easycom": {
    "autoscan": true,
    "custom": {
      "^fg-(.*)": "@/components/fg-$1/fg-$1.vue",
      "^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue",
      "^(?!z-paging-refresh|z-paging-load-more)z-paging(.*)": "z-paging/components/z-paging$1/z-paging$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/index/index",
      "type": "home",
      "layout": "tabbar",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "首页",
        "backgroundColor": "#6EA6F6"
      }
    },
    {
      "path": "pages/about/about",
      "type": "page",
      "layout": "tabbar",
      "style": {
        "navigationBarTitleText": "关于"
      }
    },
    {
      "path": "pages/about/alova",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationBarTitleText": "Alova 请求演示"
      }
    },
    {
      "path": "pages/login/login",
      "type": "page",
      "layout": "default",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "登录"
      }
    }
  ],
  "subPackages": [
    {
      "root": "pages-car",
      "pages": [
        {
          "path": "addMaster/index",
          "type": "page",
          "style": {
            "navigationStyle": "default",
            "navigationBarTitleText": "车主信息",
            "navigationBarBackgroundColor": "#ffffff"
          }
        },
        {
          "path": "addMember/index",
          "type": "page",
          "style": {
            "navigationStyle": "default",
            "navigationBarTitleText": "成员信息",
            "navigationBarBackgroundColor": "#ffffff"
          }
        },
        {
          "path": "bind/index",
          "type": "page",
          "style": {
            "navigationStyle": "default",
            "navigationBarTitleText": "绑定设备",
            "navigationBarBackgroundColor": "#ffffff"
          }
        },
        {
          "path": "editCar/index",
          "type": "page",
          "style": {
            "navigationStyle": "default",
            "navigationBarTitleText": "编辑车辆",
            "navigationBarBackgroundColor": "#ffffff"
          }
        },
        {
          "path": "interaction/index",
          "type": "page",
          "style": {
            "navigationStyle": "default",
            "navigationBarTitleText": "感应控车",
            "navigationBarBackgroundColor": "#ffffff"
          }
        },
        {
          "path": "keyBind/index",
          "type": "page",
          "style": {
            "navigationStyle": "default",
            "navigationBarTitleText": "车主信息",
            "navigationBarBackgroundColor": "#ffffff"
          }
        },
        {
          "path": "myCar/index",
          "type": "page",
          "style": {
            "navigationStyle": "default",
            "navigationBarTitleText": "我的车辆",
            "navigationBarBackgroundColor": "#ffffff"
          }
        },
        {
          "path": "shareCar/index",
          "type": "page",
          "style": {
            "navigationStyle": "default",
            "navigationBarTitleText": "分享与解绑",
            "navigationBarBackgroundColor": "#ffffff"
          }
        },
        {
          "path": "trackDetail/index",
          "type": "page",
          "layout": "default",
          "style": {
            "navigationStyle": "default",
            "navigationBarTitleText": "轨迹详情",
            "navigationBarBackgroundColor": "#ffffff"
          }
        }
      ]
    }
  ]
}
